.spec {
  display: flex;
  flex-direction: column;
  position: absolute;
  left: 10px;
  top: 10px;
  right: 10px;
  bottom: 10px;
  background-color: #fff;
  border-radius: 4px;

  .con-wrap {
    flex: 1;
    position: relative;
    z-index: 1;
    overflow-y: auto;
  }

  .tab-wrap {
    padding: 0 30px;
  }
}

.spec .j-animation {
  width: 0 !important;
}
